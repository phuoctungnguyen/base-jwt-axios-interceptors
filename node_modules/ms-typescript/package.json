{
  "name": "ms-typescript",
  "version": "2.0.0",
  "description": "Tiny millisecond conversion utility. Original package was developed by Vercel. This is reworked TypeScript version",
  "keywords":["time", "date", "ms", "milliseconds"],
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "files": [
    "/dist"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "eslint ./src/**/* --quiet --fix",
    "format": "prettier --write .",
    "test": "jest --runInBand --config jestconfig.json",
    "prepublish": "npm run lint && npm run test && npm run build"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.24.0",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.0.5",
    "prettier": "^2.3.0",
    "ts-jest": "^27.0.3",
    "typescript": "^4.2.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/grettir9/ms-typescript.git"
  },
  "author": "Ivan Z",
  "license": "MIT"
}
